{
  "name": "doto",
  "main": "index.ts",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt --hostname 0.0.0.0",
    "build": "nuxt build --spa",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nuxt/typescript": "^2.6.2",
    "@nuxtjs/axios": "^5.4.1",
    "buefy": "^0.7.3",
    "nuxt": "^2.6.2",
    "nuxt-property-decorator": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "vee-validate": "^2.2.2",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/webpack-env": "^1.13.9",
    "@nuxtjs/style-resources": "^0.1.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "5.16.0",
    "typescript": "^3.4.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "jest-serializer-vue": "^2.0.2",
    "vue-jest": "^4.0.0-beta.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "watchman": false,
    "rootDir": "./src/main/ts",
    "testRegex": ".spec.ts$",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1",
      "^~~/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "<rootDir>/**/*.vue",
      "<rootDir>/**/*.ts"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/"
  }
}
