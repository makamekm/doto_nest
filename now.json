{
    "version": 2,
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "./mt/main.ts",
            "use": "@now/node"
        }
    ],
    "env": {
        "IS_CLOUD": "1",
        "GOOGLE_CLIENT_ID": "@google-client-id",
        "GOOGLE_CLIENT_SECRET": "@google-client-secret",
        "HOST_URL": "https://doto.now.sh",
        "DB_URL": "@db-url",
        "NODE_ENV": "production"
    },
    "routes": [
        { "src": "/api/(.*)", "dest": "/mt/main.ts" }
    ],
    "github": {
        "autoJobCancelation": false
    }
}
