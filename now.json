{
    "version": 2,
    "builds": [
        {
            "src": "./src/main.ts",
            "use": "@now/node"
        }
    ],
    "env": {
        "IS_CLOUD": "1",
        "GOOGLE_CLIENT_ID": "@google-client-id",
        "GOOGLE_CLIENT_SECRET": "@google-client-secret",
        "HOST_URL": "https://doto.now.sh",
        "DB_URL": "@db-url",
        "NODE_ENV": "production",
        "PORT": 80
    },
    "routes": [
        { "src": "/.*", "dest": "/src/main.ts" }
    ],
    "github": {
        "autoJobCancelation": false
    }
}
